{
  "name": "yaroslawww/ipro-software-api-php",
  "description": "iProSoftware api client (php)",
  "keywords": [
    "iprosoftware",
    "ipro-software",
    "IproSoftware",
    "api",
    "php"
  ],
  "homepage": "https://github.com/yaroslawww/ipro-software-api-php",
  "license": "MIT",
  "authors": [
    {
      "name": "Yaroslav Georgitsa",
      "email": "yaroslav.georgitsa@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4",
    "guzzlehttp/guzzle": "~6.5",
    "nesbot/carbon": "^2.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^8.5",
    "friendsofphp/php-cs-fixer": "^2.15",
    "mikey179/vfsstream": "^2.0@dev"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "Angecode\\IproSoftware\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "src/",
      "tests/"
    ],
    "psr-4": {
      "Angecode\\IproSoftwareTests\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "format": "vendor/bin/php-cs-fixer fix"
  }
}
